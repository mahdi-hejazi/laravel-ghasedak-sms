# Copy this file to .env.testing and set your real values to run feature tests

# Your real Ghasedak API key
GHASEDAK_API_KEY=your_real_api_key_here

# Your test phone number (Iranian mobile format)
GHASEDAK_TEST_PHONE=09123456789

# Your sender number (line number)
GHASEDAK_SENDER=10008566

# Template name that has exactly ONE parameter
GHASEDAK_TEMPLATE_NAME=your_template_name

# The value to send as the first parameter to your template
GHASEDAK_TEMPLATE_PARAM1=1234

# Enable logging for debugging
GHASEDAK_LOGGING=true